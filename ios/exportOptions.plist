<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>method</key>
	<string>app-store</string>

	<!-- Let Xcode handle signing from the project settings -->
	<key>signingStyle</key>
	<string>automatic</string>

	<!-- Symbols + bitcode options -->
	<key>uploadSymbols</key>
	<true/>
	<key>uploadBitcode</key>
	<false/>
	<key>compileBitcode</key>
	<true/>

	<!-- Optional, but usually what you want in CI -->
	<key>stripSwiftSymbols</key>
	<true/>
	<key>manageAppVersionAndBuildNumber</key>
	<true/>

	<!-- OPTIONAL: if you are using manual profiles/match, you can uncomment and set a real profile name
	<key>provisioningProfiles</key>
	<dict>
		<key>YOUR.BUNDLE.ID.HERE</key>
		<string>match AppStore YOUR.BUNDLE.ID.HERE</string>
	</dict>
	-->
</dict>
</plist>
